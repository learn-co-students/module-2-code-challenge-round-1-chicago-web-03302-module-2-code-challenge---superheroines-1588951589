<h2>All Superpowers</h2>

<ul>
  <% @powers.each do |power| %>
    <li>
      <%= link_to power.name, power_path(power) %>
      <br>
      Description: <%= power.description %><br>
      Heroine Count: <%= power.heroines.count %>
    </li>
    <br>
  <% end %>
</ul>

<%= link_to "Heroines Page", heroines_path %>
