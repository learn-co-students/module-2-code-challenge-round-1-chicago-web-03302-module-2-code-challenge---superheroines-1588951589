<%= form_for @heroine do |f| %>

    <%= f.label :name %>
    <%= f.text_field :name %>
    <br>
    <%= f.label :super_name %>
    <%= f.text_field :super_name %>
    <br>
    <%= f.label :power, "Power Selection" %> <br>
    <%= f.collection_select :power_ids, Power.all, :id, :name %>
    <br>
   
    <%= f.submit "Create Heroine" %>
<% end %>