<h1>Create a Heroine</h1> <br>

<%= flash[:message] %><br>
<br>

<%= form_for @heroine do |f| %><br>
    <%= f.label :name %><br>
    <%= f.text_field :name %><br>
    <%= f.label :super_name %><br>
    <%=f.text_field :super_name %><br>
    <%= f.collection_select :power_id, Power.all, :id, :name %><br>
    <%=f.submit 'Create Heroine' %><br>
<% end %>
    
