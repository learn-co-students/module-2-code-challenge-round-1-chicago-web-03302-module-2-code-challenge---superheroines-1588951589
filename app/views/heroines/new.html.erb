<% @heroine.errors.full_messages.each do |error| %>
<%= error %><br>
<% end %>

<h1>Make a Heroine!</h1>
<%= form_for @heroine do |f| %>
<%= f.label :name %>
<%= f.text_field :name %><br>
<%= f.label :super_name %>
<%= f.text_field :super_name %><br>
<%= f.label :power_id, "Power Select"%>
<%= f.collection_select :power_id, @powers, :id, :name %><br>
<%= f.submit "Heroine, Awaaaaaay"%>
<% end %>